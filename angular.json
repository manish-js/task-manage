{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "task-manager": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/task-manager",
      "sourceRoot": "projects/task-manager/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/task-manager",
            "index": "projects/task-manager/src/index.html",
            "main": "projects/task-manager/src/main.ts",
            "polyfills": "projects/task-manager/src/polyfills.ts",
            "tsConfig": "projects/task-manager/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/task-manager/src/favicon.ico",
              "projects/task-manager/src/assets"
            ],
            "styles": [
              "projects/task-manager/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/task-manager/src/environments/environment.ts",
                  "with": "projects/task-manager/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "task-manager:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "task-manager:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "task-manager:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/task-manager/src/test.ts",
            "polyfills": "projects/task-manager/src/polyfills.ts",
            "tsConfig": "projects/task-manager/tsconfig.spec.json",
            "karmaConfig": "projects/task-manager/karma.conf.js",
            "assets": [
              "projects/task-manager/src/favicon.ico",
              "projects/task-manager/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/pink-bluegrey.css",
              "projects/task-manager/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/task-manager/**/*.ts",
              "projects/task-manager/**/*.html"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/task-manager/e2e/protractor.conf.js",
            "devServerTarget": "task-manager:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "task-manager:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "task-manager"
}
